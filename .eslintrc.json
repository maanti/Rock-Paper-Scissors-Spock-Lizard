{
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "jsx-a11y/label-has-for": [ 2, {
    "components": [ "Label" ],
      "linebreak-style": "off",
    "required": {
      "some": [ "nesting", "id" ]
    },
    "allowChildren": false
  }]
  }
}
